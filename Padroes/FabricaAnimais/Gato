public class Gato extends Animal{

    @Override
    public void criar() {
        System.out.println("Gato criado");
    }

    @Override
    public void acao() {
        System.out.println("O  gato mia");
    }

    @Override
    public void cor(String cor) {
        System.out.println("A cor do gato Ã©: " + cor);
    }

    @Override
    public Animal clonar() {
        return new Gato();
    }
}
